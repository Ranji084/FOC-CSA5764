#include <stdio.h>

int main() {

    int n, i;

    long long factorial = 1;

    printf("Enter a positive integer: ");

    scanf("%d", &n);

    // calculate factorial

    for(i=1; i<=n; i++) {

        factorial *= i;

    }

    printf("Factorial of %d = %lld", n, factorial);

    return 0;

}
