#include <stdio.h>

int main() {

    int n, digit;

    

    printf("Enter an integer number: ");

    scanf("%d", &n);

    

    printf("The digits of the number are: ");

    

    // Extract digits from right to left

    while (n > 0) {

        digit = n % 10;

        printf("%d ", digit);

        n /= 10;

    }

    

    return 0;

}

