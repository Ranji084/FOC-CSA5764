#include <stdio.h>

int main()

{

    int n, i, max;

    printf("Enter the number of integers: ");

    scanf("%d", &n);

    printf("Enter integer 1: ");

    scanf("%d", &max);

    for (i = 2; i <= n; i++)

    {

        int num;

        printf("Enter integer %d: ", i);

        scanf("%d", &num);

        if (num > max)

            max = num;

    }

    printf("The largest integer is: %d", max);

    return 0;

}

