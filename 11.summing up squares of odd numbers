#include <stdio.h>

int main() {

   int n, sum = 0;

   printf("Enter the number of odd numbers you want to sum: ");

   scanf("%d", &n);

   for (int i = 1; i <= n*2; i++) {

      if (i % 2 != 0) {  // check if i is odd

         sum += i*i;     // add the square of i to sum

      }

   }

   printf("The sum of squares of the first %d odd numbers is %d\n", n, sum);

   return 0;

}

